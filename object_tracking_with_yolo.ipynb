      "source": [
        "import os\n",
        "import cv2\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from ultralytics import YOLO\n",
        "from IPython.display import display, HTML, clear_output\n",
        "from base64 import b64encode\n",
        "import ipywidgets as widgets\n",
        "from collections import deque, defaultdict\n",
        "import math\n",
        "import shutil\n",
        "from utils.parser import get_config\n",
        "from deep_sort import DeepSort\n",
        "\n",
        "# --- KONFIGURASI UTAMA ---\n",
        "CONFIG = {\n",
        "    \"YOLO_MODEL\": \"yolov8l.pt\",           # Model deteksi objek YOLOv8 yang akan digunakan. 'l' (large) dipilih untuk akurasi yang baik.\n",
        "    \"DEEPSORT_MAX_AGE\": 30,               # Jumlah frame maksimum di mana sebuah track ID akan dipertahankan setelah objek tidak lagi terdeteksi.\n",
        "    \"COUNTING_LINE_Y\": 380,               # Posisi vertikal (koordinat Y) dari garis virtual untuk menghitung kendaraan.\n",
        "    \"PPM_RATIO\": 30.0,                    # Pixel Per Meter. Nilai kalibrasi untuk mengkonversi jarak dalam piksel ke meter.\n",
        "    \"ICON_SIZE\": 35,                      # Ukuran (dalam piksel) untuk ikon kendaraan yang ditampilkan di OSD (On-Screen Display).\n",
        "    \"CLASSES_TO_DETECT\": [2, 3, 5, 7]     # [2: mobil, 3: motor, 5: bus, 7: truk]. Kelas lain akan diabaikan.\n",
        "}\n",
        "\n",
        "# --- INISIALISASI VARIABEL GLOBAL ---\n",
        "data_deque = {}                   # Menyimpan histori posisi untuk setiap ID lacak (untuk menggambar jejak & menghitung kecepatan).\n",
        "icons = {}                        # Dictionary untuk menyimpan gambar ikon kendaraan yang sudah dimuat.\n",
        "counts_up = defaultdict(int)      # Menghitung kendaraan yang bergerak ke arah 'Naik'.\n",
        "counts_down = defaultdict(int)    # Menghitung kendaraan yang bergerak ke arah 'Turun'.\n",
        "model = None                      # Variabel untuk menampung model YOLO yang dimuat.\n",
        "cap = None                        # Variabel untuk objek VideoCapture dari OpenCV.\n",
        "DRIVE_PROJECT_PATH = '/content/drive/MyDrive/Proyek_Object_Tracking/'\n",
        "\n",
        "# --- WARNA KOTAK PEMBATAS PER KELAS (BGR) ---\n",
        "CLASS_COLORS = {\n",
        "    'car': (0, 0, 255), 'motorcycle': (0, 255, 0),\n",
        "    'bus': (255, 165, 0), 'truck': (255, 0, 255)\n",
        "}\n",
        "\n",
        "# --- FUNGSI-FUNGSI BANTUAN ---\n",
        "\n",
        "def load_icons():\n",
        "    \"\"\"Memuat file ikon kendaraan dari Google Drive.\"\"\"\n",
        "    global icons\n",
        "    icon_path = os.path.join(DRIVE_PROJECT_PATH, 'icons/')\n",
        "    icon_files = {'car': 'car.png', 'motorcycle': 'motorcycle.png', 'bus': 'bus.png', 'truck': 'truck.png'}\n",
        "    for name, filename in icon_files.items():\n",
        "        path = os.path.join(icon_path, filename)\n",
        "        if os.path.exists(path):\n",
        "            icon = cv2.imread(path, cv2.IMREAD_UNCHANGED) # IMREAD_UNCHANGED untuk membaca channel alpha (transparansi)\n",
        "            if icon is not None: icons[name] = cv2.resize(icon, (CONFIG[\"ICON_SIZE\"], CONFIG[\"ICON_SIZE\"]))\n",
        "        else: print(f\"Peringatan: File ikon tidak ditemukan di {path}\")\n",
        "\n",
        "def overlay_icon(background, icon, x, y):\n",
        "    \"\"\"Menempelkan ikon dengan transparansi ke gambar background.\"\"\"\n",
        "    h, w = icon.shape[:2]\n",
        "    if y < 0 or x < 0 or y + h > background.shape[0] or x + w > background.shape[1]: return\n",
        "    # Memeriksa apakah ikon memiliki channel alpha (transparansi)\n",
        "    if icon.shape[2] == 4:\n",
        "        alpha = icon[:, :, 3] / 255.0\n",
        "        alpha_inv = 1.0 - alpha\n",
        "        for c in range(3):\n",
        "            background[y:y+h, x:x+w, c] = (alpha * icon[:, :, c] + alpha_inv * background[y:y+h, x:x+w, c])\n",
        "\n",
        "def draw_osd(img):\n",
        "    \"\"\"\n",
        "    Menggambar On-Screen Display (OSD) yang berisi informasi hitungan.\n",
        "    Fungsi ini bertanggung jawab untuk membuat panel informasi semi-transparan di pojok kiri atas video.\n",
        "    Panel ini menampilkan jumlah total kendaraan untuk setiap kelas yang telah dihitung,\n",
        "    dipisahkan berdasarkan arah gerak (Naik dan Turun).\n",
        "    \"\"\"\n",
        "    # Jika tidak ada data hitungan sama sekali, jangan gambar apa-apa\n",
        "    if not counts_up and not counts_down:\n",
        "        return\n",
        "\n",
        "    osd_x, osd_y, pad, line_height = 20, 20, 10, CONFIG[\"ICON_SIZE\"] + 10\n",
        "\n",
        "\n",
        "    # 1. Atur lebar OSD menjadi nilai tetap yang lebih sesuai (bisa diubah)\n",
        "    osd_w = 250\n",
        "\n",
        "    # 2. Hitung tinggi OSD secara dinamis berdasarkan konten\n",
        "    total_h = pad  # Mulai dengan padding atas\n",
        "\n",
        "    if counts_up:\n",
        "        total_h += line_height  # Tinggi untuk judul \"ARAH NAIK\"\n",
        "        total_h += len(counts_up) * line_height  # Tinggi untuk setiap item di 'counts_up'\n",
        "\n",
        "    if counts_down:\n",
        "        if counts_up:\n",
        "            total_h += pad  # Beri jarak jika 'counts_up' juga ada\n",
        "        total_h += line_height  # Tinggi untuk judul \"ARAH TURUN\"\n",
        "        total_h += len(counts_down) * line_height  # Tinggi untuk setiap item di 'counts_down'\n",
        "\n",
        "    total_h += pad # Selesaikan dengan padding bawah\n",
        "    osd_h = total_h\n",
        "\n",
        "    # Pastikan OSD tidak keluar dari layar\n",
        "    osd_h = min(osd_h, img.shape[0] - osd_y)\n",
        "    osd_w = min(osd_w, img.shape[1] - osd_x)\n",
        "\n",
        "    # Membuat area OSD menjadi semi-transparan\n",
        "    sub_img = img[osd_y:osd_y+osd_h, osd_x:osd_x+osd_w]\n",
        "    black_rect = np.zeros(sub_img.shape, dtype=np.uint8)\n",
        "    res = cv2.addWeighted(sub_img, 0.6, black_rect, 0.4, 1.0)\n",
        "    img[osd_y:osd_y+osd_h, osd_x:osd_x+osd_w] = res\n",
        "\n",
        "    # Menulis teks dan ikon untuk hitungan 'ARAH NAIK'\n",
        "    curr_y = osd_y + pad\n",
        "    if counts_up:\n",
        "        cv2.putText(img, \"ARAH NAIK\", (osd_x + pad, curr_y + 25), cv2.FONT_HERSHEY_SIMPLEX, 0.7, (255, 255, 0), 2)\n",
        "        curr_y += line_height\n",
        "        for cls, val in sorted(counts_up.items()):\n",
        "            obj_name = model.names[cls]\n",
        "            if obj_name in icons: overlay_icon(img, icons[obj_name], osd_x + pad, curr_y)\n",
        "            text = f\": {obj_name.capitalize()} - {val}\"\n",
        "            cv2.putText(img, text, (osd_x + pad + CONFIG[\"ICON_SIZE\"], curr_y + 25), cv2.FONT_HERSHEY_SIMPLEX, 0.6, (255, 255, 255), 1)\n",
        "            curr_y += line_height\n",
        "\n",
        "    # Menulis teks dan ikon untuk hitungan 'ARAH TURUN'\n",
        "    if counts_down:\n",
        "        if counts_up:\n",
        "            curr_y += pad # Beri jarak\n",
        "        cv2.putText(img, \"ARAH TURUN\", (osd_x + pad, curr_y + 25), cv2.FONT_HERSHEY_SIMPLEX, 0.7, (0, 255, 255), 2)\n",
        "        curr_y += line_height\n",
        "        for cls, val in sorted(counts_down.items()):\n",
        "            obj_name = model.names[cls]\n",
        "            if obj_name in icons: overlay_icon(img, icons[obj_name], osd_x + pad, curr_y)\n",
        "            text = f\": {obj_name.capitalize()} - {val}\"\n",
        "            cv2.putText(img, text, (osd_x + pad + CONFIG[\"ICON_SIZE\"], curr_y + 25), cv2.FONT_HERSHEY_SIMPLEX, 0.6, (255, 255, 255), 1)\n",
        "            curr_y += line_height\n",
        "\n",
        "def process_frame(frame, tracker, fps, counted_ids_set):\n",
        "    \"\"\"\n",
        "    Fungsi inti yang memproses setiap frame dari video.\n",
        "    Ini adalah alur kerja utama per frame: deteksi -> pelacakan -> visualisasi -> penghitungan.\n",
        "    \"\"\"\n",
        "    line_y = CONFIG[\"COUNTING_LINE_Y\"]\n",
        "    # 1. Gambar garis hitung virtual pada frame.\n",
        "    cv2.line(frame, (0, line_y), (frame.shape[1], line_y), (0, 255, 255), 2)\n",
        "\n",
        "    # 2. Lakukan deteksi objek pada frame menggunakan model YOLO.\n",
        "    results = model(frame, conf=conf_slider.value, classes=CONFIG[\"CLASSES_TO_DETECT\"], verbose=False)\n",
        "    xywhs, confs, clss = [], [], []\n",
        "    for r in results:\n",
        "        for box in r.boxes:\n",
        "            x1, y1, x2, y2 = box.xyxy[0].cpu().numpy()\n",
        "            # Konversi format [x1, y1, x2, y2] ke [center_x, center_y, width, height] yang dibutuhkan DeepSORT.\n",
        "            xywhs.append([(x1+x2)/2, (y1+y2)/2, x2-x1, y2-y1])\n",
        "            confs.append(float(box.conf[0])); clss.append(int(box.cls[0]))\n",
        "\n",
        "    # 3. Kirim hasil deteksi ke tracker DeepSORT.\n",
        "    # Tracker akan mengembalikan ID unik dan posisi yang telah diperbarui untuk setiap objek.\n",
        "    if len(xywhs) > 0:\n",
        "        tracks = tracker.update(np.array(xywhs), np.array(confs), np.array(clss), frame)\n",
        "        if len(tracks) > 0:\n",
        "            # 4. Gambar hasil pelacakan (kotak, ID, jejak).\n",
        "            draw_tracks(frame, tracks, fps)\n",
        "            # 5. Periksa apakah ada objek yang melintasi garis hitung.\n",
        "            update_counts(frame, tracks, counted_ids_set)\n",
        "\n",
        "    # 6. Gambar OSD di atas frame yang sudah diproses.\n",
        "    draw_osd(frame)\n",
        "    return frame\n",
        "\n",
        "def draw_tracks(img, tracks, fps):\n",
        "    \"\"\"Menggambar kotak pembatas, jejak, dan estimasi kecepatan untuk setiap objek yang dilacak.\"\"\"\n",
        "    for track in tracks:\n",
        "        x1, y1, x2, y2 = map(int, track[:4])\n",
        "        track_id, obj_id = int(track[4]), int(track[5])\n",
        "        obj_name = model.names[obj_id]\n",
        "        color = CLASS_COLORS.get(obj_name, (200, 200, 200)) # Default warna abu-abu\n",
        "\n",
        "        # Gunakan `deque` (antrean dua ujung) dengan panjang maksimal untuk menyimpan histori posisi.\n",
        "        # Ini efisien untuk melacak jejak dan menghitung kecepatan.\n",
        "        if track_id not in data_deque: data_deque[track_id] = deque(maxlen=64)\n",
        "        center = (int((x1 + x2) / 2), int((y1 + y2) / 2))\n",
        "        data_deque[track_id].appendleft({'center': center, 'time': cap.get(cv2.CAP_PROP_POS_FRAMES)})\n",
        "\n",
        "        # Estimasi kecepatan.\n",
        "        # Menghitung jarak piksel antara posisi saat ini dan posisi 10 frame sebelumnya,\n",
        "        # mengkonversinya ke meter menggunakan PPM_RATIO, lalu menghitung kecepatan dalam km/jam.\n",
        "        speed_kmh = 0\n",
        "        if len(data_deque[track_id]) >= 10:\n",
        "            p1_data, p2_data = data_deque[track_id][0], data_deque[track_id][9] # Bandingkan frame saat ini dengan 9 frame sebelumnya\n",
        "            time_delta = (p1_data['time'] - p2_data['time']) / fps\n",
        "            if time_delta > 0:\n",
        "                pixel_dist = math.sqrt((p1_data['center'][0] - p2_data['center'][0])**2 + (p1_data['center'][1] - p2_data['center'][1])**2)\n",
        "                speed_kmh = (pixel_dist / CONFIG[\"PPM_RATIO\"] / time_delta) * 3.6 # Konversi m/s ke km/jam\n",
        "\n",
        "        # Gambar kotak pembatas (bounding box) dan labelnya.\n",
        "        cv2.rectangle(img, (x1, y1), (x2, y2), color, 2)\n",
        "        label = f\"{obj_name.capitalize()} ID:{track_id} | {speed_kmh:.1f} km/j\"\n",
        "        (w, h), _ = cv2.getTextSize(label, cv2.FONT_HERSHEY_SIMPLEX, 0.5, 1)\n",
        "        cv2.rectangle(img, (x1, y1 - h - 10), (x1 + w, y1), color, -1)\n",
        "        cv2.putText(img, label, (x1, y1 - 5), cv2.FONT_HERSHEY_SIMPLEX, 0.5, (255, 255, 255), 2)\n",
        "\n",
        "        # Gambar jejak (garis yang mengikuti pergerakan objek)\n",
        "        for i in range(1, len(data_deque[track_id])):\n",
        "            thickness = int(np.sqrt(64 / float(i + 1)) * 1.5) # Jejak memudar seiring waktu\n",
        "            cv2.line(img, data_deque[track_id][i-1]['center'], data_deque[track_id][i]['center'], color, thickness)\n",
        "\n",
        "\n",
        "def update_counts(frame, tracks, counted_ids_set):\n",
        "    \"\"\"\n",
        "    Memperbarui hitungan jika objek melintasi garis. Ini adalah logika utama untuk penghitungan.\n",
        "    \"\"\"\n",
        "    line_y = CONFIG[\"COUNTING_LINE_Y\"]\n",
        "    for track in tracks:\n",
        "        track_id, obj_id = int(track[4]), int(track[5])\n",
        "\n",
        "        # Pastikan setiap kendaraan hanya dihitung SATU KALI saja selama pelacakannya.\n",
        "        # Gunakan titik tengah bawah (bottom center) untuk logika penghitungan\n",
        "        if track_id not in counted_ids_set and len(data_deque.get(track_id, [])) > 1:\n",
        "            # Ambil posisi Y saat ini dan posisi Y pada frame sebelumnya dari titik tengah bawah.\n",
        "            current_y = data_deque[track_id][0]['center'][1]\n",
        "            prev_y = data_deque[track_id][1]['center'][1]\n",
        "\n",
        "            # Deteksi persilangan garis untuk 'Arah Turun'.\n",
        "            # Jika posisi sebelumnya di ATAS garis dan posisi saat ini di BAWAH garis.\n",
        "            if prev_y < line_y and current_y >= line_y:\n",
        "                counts_down[obj_id] += 1\n",
        "                counted_ids_set.add(track_id)\n",
        "                # Ubah warna garis menjadi merah sesaat sebagai umpan balik visual.\n",
        "                cv2.line(frame, (0, line_y), (frame.shape[1], line_y), (0, 0, 255), 4)\n",
        "\n",
        "            # Deteksi persilangan garis untuk 'Arah Naik'.\n",
        "            # Jika posisi sebelumnya di BAWAH garis dan posisi saat ini di ATAS garis.\n",
        "            elif prev_y > line_y and current_y <= line_y:\n",
        "                counts_up[obj_id] += 1\n",
        "                counted_ids_set.add(track_id)\n",
        "                # Ubah warna garis menjadi merah sesaat sebagai umpan balik visual.\n",
        "                cv2.line(frame, (0, line_y), (frame.shape[1], line_y), (0, 0, 255), 4)\n",
        "\n",
        "def show_video(video_path, width=900):\n",
        "    \"\"\"Menampilkan video di output notebook.\"\"\"\n",
        "    try:\n",
        "        video_file = open(video_path, \"rb\").read()\n",
        "        video_url = f\"data:video/mp4;base64,{b64encode(video_file).decode()}\"\n",
        "        return HTML(f'<video width=\"{width}\" controls autoplay loop><source src=\"{video_url}\"></video>')\n",
        "    except FileNotFoundError: return HTML(f\"<p style='color:red;'> Error: File video tidak ditemukan.</p>\")\n",
        "\n",
        "def save_to_excel(video_name):\n",
        "    \"\"\"\n",
        "    Menyusun data dan menyimpannya ke file Excel dengan format yang rapi.\n",
        "    Fungsi ini mengumpulkan semua data hitungan, merangkumnya dalam DataFrame Pandas,\n",
        "    menambahkan baris total, dan menyimpannya ke dalam file .xlsx.\n",
        "    \"\"\"\n",
        "    all_detected_classes = set(counts_up.keys()) | set(counts_down.keys())\n",
        "    if not all_detected_classes:\n",
        "        print(\"Tidak ada kendaraan terhitung, file Excel tidak dibuat.\")\n",
        "        return\n",
        "\n",
        "    # Siapkan data untuk DataFrame\n",
        "    summary_data = []\n",
        "    for obj_id in sorted(list(all_detected_classes)):\n",
        "        class_name = model.names.get(obj_id, f\"ClassID_{obj_id}\").capitalize()\n",
        "        summary_data.append({\n",
        "            'Jenis Kendaraan': class_name,\n",
        "            'Arah Naik': counts_up.get(obj_id, 0),\n",
        "            'Arah Turun': counts_down.get(obj_id, 0)\n",
        "        })\n",
        "\n",
        "    df_summary = pd.DataFrame(summary_data)\n",
        "\n",
        "    # Tambahkan baris Total\n",
        "    total_row = pd.DataFrame({\n",
        "        'Jenis Kendaraan': ['**TOTAL**'],\n",
        "        'Arah Naik': [df_summary['Arah Naik'].sum()],\n",
        "        'Arah Turun': [df_summary['Arah Turun'].sum()]\n",
        "    })\n",
        "    df_summary = pd.concat([df_summary, total_row], ignore_index=True)\n",
        "\n",
        "    # Tentukan path dan simpan file Excel\n",
        "    excel_filename = f\"Laporan_{os.path.splitext(video_name)[0]}.xlsx\"\n",
        "    EXCEL_OUTPUT_PATH = os.path.join(OUTPUT_DIR, excel_filename)\n",
        "    df_summary.to_excel(EXCEL_OUTPUT_PATH, index=False, sheet_name='Ringkasan Analisis')\n",
        "    print(f\"Laporan Excel berhasil disimpan di: {EXCEL_OUTPUT_PATH}\")\n",
        "\n",
        "\n",
        "# --- UI WIDGETS & FUNGSI EKSEKUSI ---\n",
        "VIDEO_DIR = os.path.join(DRIVE_PROJECT_PATH, 'videos/')\n",
        "OUTPUT_DIR = os.path.join(DRIVE_PROJECT_PATH, 'output/')\n",
        "os.makedirs(VIDEO_DIR, exist_ok=True); os.makedirs(OUTPUT_DIR, exist_ok=True)\n",
        "try:\n",
        "    video_files = [f for f in os.listdir(VIDEO_DIR) if f.endswith(('.mp4', '.avi', '.mov'))]\n",
        "    if not video_files: video_files = ['Tidak ada file video di folder.']\n",
        "except FileNotFoundError: video_files = [f'Error: Folder {VIDEO_DIR} tidak ditemukan.']\n",
        "\n",
        "video_selector = widgets.Dropdown(options=video_files, description='Pilih Video:', layout={'width': '500px'})\n",
        "conf_slider = widgets.FloatSlider(value=0.6, min=0.1, max=1.0, step=0.05, description='Confidence:')\n",
        "run_button = widgets.Button(description='Mulai Proses', button_style='success', icon='play')\n",
        "output_widget = widgets.Output()\n",
        "\n",
        "def run_processing(b):\n",
        "    \"\"\"\n",
        "    Fungsi utama yang dieksekusi saat tombol 'Mulai Proses' ditekan.\n",
        "    Ini adalah 'otak' dari keseluruhan proses, yang mengorkestrasi semua langkah:\n",
        "    1. Inisialisasi dan pembersihan\n",
        "    2. Memuat model dan video\n",
        "    3. Memproses video frame-by-frame\n",
        "    4. Menyimpan hasil video dan laporan Excel\n",
        "    5. Menampilkan hasil\n",
        "    \"\"\"\n",
        "    global model, cap, data_deque, counts_up, counts_down\n",
        "    with output_widget:\n",
        "        clear_output(wait=True)\n",
        "        # 1. Inisialisasi ulang variabel global untuk setiap pemrosesan baru\n",
        "        print(\"Mempersiapkan pemrosesan...\"); data_deque.clear(); counts_up.clear(); counts_down.clear(); load_icons()\n",
        "        video_name = video_selector.value; VIDEO_IN_PATH = os.path.join(VIDEO_DIR, video_name)\n",
